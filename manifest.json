{
  "manifest_version": 3,
  "name": "Pomodoro Timer",
  "version": "1.0",
  "description": "Pomodoro Timer for Google Chrome",
  "author": "Omar Belghaouti",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_title": "Pomodoro Timer",
    "default_icon": "icon.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage"]
}
